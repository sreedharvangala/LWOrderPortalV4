    <field name="CompanyName" type="String" readOnly="true" label="Company" length="50" />
    <field name="CustSysRowID" type="Guid" allowNulls="false" label="Customer" showInSummary="true" contextFields="Company=CompanyName,ExCurUser=CurUser" causesCalculate="true">
